{
  "name": "path-of-exile-item-filter",
  "main": "./dist/main",
  "version": "1.1.6",
  "description": "An Atom package designed to make editing Path of Exile item filters easier.",
  "keywords": [
    "poe",
    "exile",
    "filter",
    "loot",
    "linter"
  ],
  "activationCommands": {},
  "repository": "https://github.com/atom-exiles/path-of-exile-item-filter",
  "license": "MIT",
  "scripts": {
    "test": "atom --test test",
    "lint": "eslint --max-warnings 0 test lib"
  },
  "engines": {
    "atom": ">=1.18.0 <2.0.0"
  },
  "atomTestRunner": "./test/runner",
  "atomTranspilers": [
    {
      "glob": "{lib,test}/**/*.js",
      "transpiler": "atom-babel6-transpiler",
      "options": {
        "cacheKeyFiles": [
          "package.json",
          ".babelrc"
        ]
      }
    }
  ],
  "dependencies": {
    "atom-babel6-transpiler": "^1.1.2",
    "atom-package-deps": "^4.6.0",
    "babel-generator": "^6.26.0",
    "babel-plugin-chai-assert-async": "^0.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "classnames": "^2.2.5",
    "etch": "^0.12.5",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.74",
    "atom-mocha-test-runner": "^1.0.1",
    "atom-typings": "^1.19.0",
    "babel-eslint": "^7.2.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^4.5.0",
    "eslint-config-fbjs": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-relay": "0.0.15",
    "mocha": "^3.5.0",
    "mocha-appveyor-reporter": "^0.4.0",
    "mocha-junit-and-console-reporter": "^1.6.0",
    "test-until": "^1.1.1"
  },
  "package-deps": [
    "linter:2.2.0"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideCompletion"
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinter"
      }
    }
  }
}
